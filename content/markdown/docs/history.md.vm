#macro ( release $date $version $announce $bold $jdk $rowspan )
#if( $bold.equals( "true" ) )
#set ( $b = '<b>' )
#set ( $b_ = '</b>' )
#else
#set ( $b = '' )
#set ( $b_ = '' )
#end
<tr><td>$b$date$b_</td><td>$b$version$b_</td>##
#if( $jdk.length() > 0 )
<td rowspan="$rowspan">$jdk</td>##
#end
<td>##
#if( !$announce.equals( "" ) )
